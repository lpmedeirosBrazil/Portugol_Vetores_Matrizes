funcao inicio()
	{
		inteiro contador=0
		cadeia nomes[][]={{"João","São Paulo","(11) 9999-5241"},{"Maria","Ribeirão Preto","(16) 9999-8596"},{"Ana","Manaus","(92) 9999-8574"}}

		faca{

			escreva ("Nome: " + nomes[contador][0] + " Cidade: " + nomes[contador][1] + " Celular: " + nomes[contador][2] + "\n")
			contador++
			
		}enquanto (contador<=2)
	}
